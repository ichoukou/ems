package com.channelsoft.ems.po;
/**
 *  月份分析的PO类
 	* @author lizhu
 	* @Copyright Channelsoft
	* @2017年3月1日
 */
public class AnalysePo {

	//年月
	private String period;
	//收入金额
	private double fInAmount;
	//支出金额
	private double fOutAmount;
	//差额
	private double changeAmount;
	
	public AnalysePo(){
		
	}
	
	public AnalysePo(String period,double fInAmount,double fOutAmount,double changeAmount){
		this.period=period;
		this.fInAmount=fInAmount;
		this.fOutAmount=fOutAmount;
		this.changeAmount=changeAmount;
	}
	
	public String toString(){
		return "AnalysePo{"
				+ "period='"+this.period+"',"
				+ "fInAmount='"+this.fInAmount+"',"
				+ "fOutAmount='"+this.fOutAmount+"',"
				+ "changeAmount='"+this.changeAmount+"'"
				+ "}";
	}

	public String getPeriod() {
		return period;
	}

	public void setPeriod(String period) {
		this.period = period;
	}

	public double getfInAmount() {
		return fInAmount;
	}

	public void setfInAmount(double fInAmount) {
		this.fInAmount = fInAmount;
	}

	public double getfOutAmount() {
		return fOutAmount;
	}

	public void setfOutAmount(double fOutAmount) {
		this.fOutAmount = fOutAmount;
	}

	public double getChangeAmount() {
		return changeAmount;
	}

	public void setChangeAmount(double changeAmount) {
		this.changeAmount = changeAmount;
	}
	
	
	

}

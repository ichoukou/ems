<%@ page language="java" import="java.util.*" pageEncoding="utf-8" isELIgnored="false"%>

<%-- <!-- ajax 文件提交 start -->
<script type="text/javascript" src="${ctx}static/common/javascript/jquery.form.js"></script> 
<!-- ajax 文件提交 end --> --%>

<div class="container-fluid" style="font-size: 11px;" ng-app="jobAddApp" id="jobadd">
	<div class="row" ng-controller="jobAddCtrl">
		<div class="col-md-12">
	
			<form class="form-horizontal" id="add_form" action="#" role="form" enctype="multipart/form-data">
			
				<div class="form-group">
					<label for="name" class="col-sm-2 control-label">任务名</label>
					<div class="col-sm-6">
						<input type="text" class="form-control" id="add_jobname" name="add_jobname" check-type="required" required-message="请填写任务名。">
					</div>
				</div>
			
				<div class="form-group">
					<label for="name" class="col-sm-2 control-label">时间表达式</label>
					<div class="col-sm-6">
						<input type="text" class="form-control" id="add_cronexpression" name="add_cronexpression" check-type="required" required-message="请填写时间表达式。">
					</div>
				</div>
				
				<div class="form-group">
					<label for="name" class="col-sm-2 control-label">触发器名</label>
					<div class="col-sm-6">
						<input type="text" class="form-control" id="add_triggername" name="add_triggername" check-type="required" required-message="请填写触发器名。">
					</div>
				</div>
			
				<div class="form-group">
					<label for="pw1" class="col-sm-2 control-label">执行类</label>
					<div class="col-sm-6">
						<input type="text" class="form-control input-sm" id="add_jobclass" name="add_jobclass" check-type="required" required-message="请填写执行类。">
					</div>
				</div>
					
				<div class="form-group">
					<label for="vercode" class="col-sm-2 control-label">备注</label>
					<div class="col-sm-6">
						<textarea class="form-control" id="add_description" name="add_description"></textarea>
					</div>
				</div>
				
<!-- 				<div class="form-group">
					<label for="vercode" class="col-sm-2 control-label">自定义参数</label>
					<div class="col-sm-6">
						<textarea class="form-control" id="customParams" name="customParams" placeholder="请按照Json格式填写."></textarea>
					</div>
				</div> -->
			</form>

		</div>
	</div>
</div>


